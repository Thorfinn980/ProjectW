resource_name = "attackHitbox"
script/source = "extends Area2D
var player: CharacterBody2D
var oni: CharacterBody2D
var do_damage: bool = false
var damage = 10
# Called when the node enters the scene tree for the first time.
func _ready():
	player = get_tree().get_first_node_in_group(\"Player\")
	oni = get_tree().get_first_node_in_group(\"Oni\")
	

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	pass
	# flips hitbox
	#if oni.direction.x < 0:
		#scale.x = -1
	#else:
		#scale.x = 1

func do_some_damage():
	if do_damage:
		var children = player.get_children()
		for kid in children:
			if kid is Damageable:
				kid.hit(10)

func _on_body_entered(body):
	if body.name == \"Player\":
		do_damage = true

func _on_body_exited(body):
	if body.name == \"Player\":
		do_damage = false
"

